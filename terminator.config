[global_config]
[keybindings]
  split_horiz = <Primary><Shift>d
  split_vert = <Primary>d
  toggle_zoom = <Primary>Return
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
