[global_config]
  enabled_plugins = Logger, LaunchpadBugURLHandler, LaunchpadCodeURLHandler
  suppress_multiple_term_dialog = True
  case_sensitive = False
[keybindings]
  new_tab = <Primary>t
  split_horiz = <Primary><Shift>d
  split_vert = <Primary>d
  toggle_zoom = <Primary>Return
  next_tab = <Primary>Right
  prev_tab = <Primary>Left
[profiles]
  [[default]]
    font = Roboto Mono for Powerline Bold 10
    scrollback_lines = 10000
    use_system_font = False
  [[presenting]]
    font = Source Code Pro for Powerline Semi-Bold 16
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
